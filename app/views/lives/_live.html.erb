<li id="live-<%= live.id %>">
  <%= link_to live.picture, life_path(live.id) %>
    <span class="date"><%= live.date %></span>
    <span class="place"><%= live.place %></span>
    <span class="title"><%= live.title %></span>
    <span class="time"><%= live.time %></span>
    <span class="price"><%= live.price %></span>
    <span class="performer"><%= live.performer %></span>
</li>
